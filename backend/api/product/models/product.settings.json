{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "price": {
      "type": "float"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "Custom_field": {
      "type": "component",
      "repeatable": true,
      "component": "custom.custom-field"
    },
    "bundle": {
      "via": "products",
      "model": "bundle"
    },
    "categories": {
      "via": "products",
      "collection": "category",
      "dominant": true
    },
    "included": {
      "type": "json"
    },
    "sales": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "width": {
      "type": "decimal",
      "default": 0,
      "required": true,
      "min": 0
    },
    "height": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "lengtt": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "volume": {
      "type": "decimal",
      "default": 0,
      "required": true,
      "min": 0
    },
    "weight": {
      "type": "decimal",
      "default": 0,
      "required": true,
      "min": 0
    }
  }
}
