{
  "routes": [
    {
      "method": "GET",
      "path": "/purchase-types",
      "handler": "purchase-type.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/purchase-types/count",
      "handler": "purchase-type.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/purchase-types/:id",
      "handler": "purchase-type.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/purchase-types",
      "handler": "purchase-type.create",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/purchase-types/:id",
      "handler": "purchase-type.update",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/purchase-types/:id",
      "handler": "purchase-type.delete",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    }
  ]
}
