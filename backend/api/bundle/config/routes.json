{
  "routes": [
    {
      "method": "GET",
      "path": "/bundles",
      "handler": "bundle.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/bundles/count",
      "handler": "bundle.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/bundles/:id",
      "handler": "bundle.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/bundles",
      "handler": "bundle.create",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/bundles/:id",
      "handler": "bundle.update",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/bundles/:ids",
      "handler": "bundle.delete",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    }
  ]
}
