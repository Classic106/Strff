{
  "routes": [
    {
      "method": "GET",
      "path": "/order-items",
      "handler": "order-item.find",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/order-items/count",
      "handler": "order-item.count",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/order-items/:id",
      "handler": "order-item.findOne",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/order-items",
      "handler": "order-item.create",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/order-items/:id",
      "handler": "order-item.update",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/order-items/:id",
      "handler": "order-item.delete",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    }
  ]
}
