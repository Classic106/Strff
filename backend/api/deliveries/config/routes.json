{
  "routes": [
    {
      "method": "GET",
      "path": "/deliveries",
      "handler": "deliveries.find",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/deliveries/count",
      "handler": "deliveries.count",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/deliveries/:id",
      "handler": "deliveries.findOne",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/deliveries",
      "handler": "deliveries.create",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/deliveries/:id",
      "handler": "deliveries.update",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/deliveries/:id",
      "handler": "deliveries.delete",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    }
  ]
}
