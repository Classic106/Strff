{
  "routes": [
    {
      "method": "GET",
      "path": "/users/get_customers",
      "handler": "user.getCustomers",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/users/:ids",
      "handler": "user.deleteMany",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/users/block/:ids",
      "handler": "user.blockMany",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/users/check_user",
      "handler": "user.checkUser",
      "config": {
        "policies": []
      }
    }
  ]
}
