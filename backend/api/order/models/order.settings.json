{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order_date": {
      "type": "datetime"
    },
    "total": {
      "type": "decimal"
    },
    "order_status": {
      "model": "order-statuses"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "order_bundles": {
      "collection": "order-bundle",
      "via": "order"
    },
    "order_items": {
      "collection": "order-item",
      "via": "order"
    },
    "paid": {
      "type": "boolean"
    },
    "customer": {
      "via": "orders",
      "model": "customer"
    }
  }
}
